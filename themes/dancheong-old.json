    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#a4aaa7",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#f7b938"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#d8c8b2"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#5eb3ff"
            }
        },
        {
            "name": "Keywords (Jangdan Red)",
            "scope": [
                "keyword",
                "keyword.control",
                "storage",
                "storage.type",
                "storage.modifier",
                "keyword.operator.new",
                "keyword.operator.expression"
            ],
            "settings": {
                "foreground": "#e16350"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#d8c8b2"
            }
        },
        {
            "name": "Language Constants (this/super)",
            "scope": [
                "variable.language",
                "constant.language"
            ],
            "settings": {
                "foreground": "#e16350",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function",
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#5eb3ff"
            }
        },
        {
            "name": "Function Parameters",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#ebe3c4",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Classes/Types",
            "scope": [
                "entity.name.type",
                "support.type",
                "support.class",
                "new.expr entity.name.type"
            ],
            "settings": {
                "foreground": "#72c6a5"
            }
        },
        {
            "name": "Numbers/Constants",
            "scope": [
                "constant.numeric",
                "constant.other",
                "support.constant"
            ],
            "settings": {
                "foreground": "#5eb3ff"
            }
        },
        {
            "name": "Attributes/Properties",
            "scope": [
                "entity.other.attribute-name",
                "variable.other.property",
                "meta.object.member"
            ],
            "settings": {
                "foreground": "#5eb3ff"
            }
        },
        {
            "name": "Tags",
            "scope": [
                "entity.name.tag",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#e16350"
            }
        },
        {
            "name": "Markdown Headings",
            "scope": [
                "markup.heading",
                "entity.name.section"
            ],
            "settings": {
                "foreground": "#5eb3ff",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Links",
            "scope": [
                "markup.underline.link",
                "string.other.link"
            ],
            "settings": {
                "foreground": "#5eb3ff"
            }
        },
        {
            "name": "Markdown Bold/Italic",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#e16350"
            }
        },
        {
            "name": "Markdown Italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#e16350"
            }
        },
        {
            "name": "Escape Characters",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#72c6a5"
            }
        }
    ],
    "semanticTokenColors": {
        "variable": "#d8c8b2",
        "function": "#5eb3ff",
        "type": "#72c6a5",
        "class": "#72c6a5",
        "interface": "#72c6a5",
        "enum": "#72c6a5",
        "parameter": "#ebe3c4",
        "property": "#5eb3ff",
        "keyword": "#e16350",
        "string": "#f7b938",
        "number": "#5eb3ff",
        "operator": "#5eb3ff"
    }
}