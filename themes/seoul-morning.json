    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#a4aaa7",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#237a4e"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#616264"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#c23352"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "keyword.control",
                "storage",
                "storage.type",
                "storage.modifier",
                "keyword.operator.new",
                "keyword.operator.expression"
            ],
            "settings": {
                "foreground": "#c23352"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#405349"
            }
        },
        {
            "name": "Language Constants",
            "scope": [
                "variable.language",
                "constant.language"
            ],
            "settings": {
                "foreground": "#c23352",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function",
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#006494"
            }
        },
        {
            "name": "Function Parameters",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#405349",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Classes/Types",
            "scope": [
                "entity.name.type",
                "support.type",
                "support.class",
                "new.expr entity.name.type"
            ],
            "settings": {
                "foreground": "#ba4160"
            }
        },
        {
            "name": "Numbers/Constants",
            "scope": [
                "constant.numeric",
                "constant.other",
                "support.constant"
            ],
            "settings": {
                "foreground": "#006494"
            }
        },
        {
            "name": "Attributes/Properties",
            "scope": [
                "entity.other.attribute-name",
                "variable.other.property",
                "meta.object.member"
            ],
            "settings": {
                "foreground": "#006494"
            }
        },
        {
            "name": "Tags",
            "scope": [
                "entity.name.tag",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#c23352"
            }
        },
        {
            "name": "Markdown Headings",
            "scope": [
                "markup.heading",
                "entity.name.section"
            ],
            "settings": {
                "foreground": "#006494",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Links",
            "scope": [
                "markup.underline.link",
                "string.other.link"
            ],
            "settings": {
                "foreground": "#0b6db7"
            }
        },
        {
            "name": "Markdown Bold/Italic",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#c23352"
            }
        },
        {
            "name": "Escape Characters",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#ba4160"
            }
        }
    ],
    "semanticTokenColors": {
        "variable": "#405349",
        "function": "#006494",
        "type": "#ba4160",
        "class": "#ba4160",
        "interface": "#ba4160",
        "enum": "#ba4160",
        "parameter": "#405349",
        "property": "#006494",
        "keyword": "#c23352",
        "string": "#237a4e",
        "number": "#006494",
        "operator": "#c23352"
    }
}